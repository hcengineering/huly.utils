{
    "extends": [
        "standard-with-typescript"
    ],
    "ignorePatterns": ["*.json", "node_modules/*", ".eslintrc.js"],
    "rules": {
        "@typescript-eslint/array-type": "off",
        "@typescript-eslint/promise-function-async": "off",
        "@typescript-eslint/consistent-type-imports": "off"
    }
}
